@model TestApp.Models.Educations

@{
    ViewData["Title"] = "EducationsAdd";
}

<form asp-action="EducationsAdd" asp-controller="Home" enctype="multipart/form-data" method="post">

    <div class="form-group">
        <label asp-for="EducationName" class="control-label">Образование</label>
        <input type="text" asp-for="EducationName" class="form-control" />
    </div>
    <br />
    <div class="form-group">
        <input type="submit" style="background-color: #66a5ad !important; color: white" value="Отправить" class="btn btn-default" />
    </div>
</form>
